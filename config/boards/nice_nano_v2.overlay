/ {
    chosen {
        /* Tell ZMK which battery device to use */
        zmk,battery = &vddh_batt;
    };

    /* Use the nRF52840 VDDH measurement (works when VDDH domain is present) */
    vddh_batt: vddh_batt {
        compatible = "zmk,battery-nrf-vddh";
        status = "okay";
    };
};
