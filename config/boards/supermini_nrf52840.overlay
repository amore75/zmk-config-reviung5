/ {
    chosen {
        /* Point ZMK at our battery device */
        zmk,battery = &bat;
    };

    /* Internal battery measurement via nRF VDDH (good for nice!nano v2) */
    bat: battery {
              compatible = "zmk,battery-nrf-vdd";

        status = "okay";
    };
};
